{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run FinalProject-EmployeeGroup",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/finalproject-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/logaliEmployeeGroup/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/EmployeeGroup",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/FinalProject/EmployeeGroup\"}"
      },
      "envFile": "${workspaceFolder}/EmployeeGroup/.env2",
      "preLaunchTask": "Build FinalProject"
    }
  ]
}